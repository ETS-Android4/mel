<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="524.0" prefWidth="755.0" xmlns="http://javafx.com/javafx/null"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mein.auth.service.MeinAuthAdminFX">
    <bottom>
        <HBox alignment="CENTER" prefHeight="23.0" prefWidth="755.0" BorderPane.alignment="CENTER">
            <Button fx:id="btnPrimary" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Apply"
                    HBox.hgrow="ALWAYS"/>
            <Button fx:id="btnSecondary" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Refresh"
                    HBox.hgrow="ALWAYS"/>
        </HBox>
    </bottom>
    <right>
        <HBox prefHeight="100.0" BorderPane.alignment="CENTER"/>
    </right>
    <center>
        <TableView fx:id="table" editable="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn prefWidth="75.0" text="C1"/>
                <TableColumn prefWidth="75.0" text="C2"/>
            </columns>
        </TableView>
    </center>
    <top>
        <TilePane prefHeight="0.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <HBox prefHeight="0.0" prefWidth="200.0">
                <Label text="filter Services"/>
                <TextField fx:id="txtServiceFilter"/>
            </HBox>
            <HBox prefHeight="0.0" prefWidth="200.0">
                <Label text="filter Certificates"/>
                <TextField fx:id="txtCertificateFilter"/>
            </HBox>
        </TilePane>
    </top>
</BorderPane>
