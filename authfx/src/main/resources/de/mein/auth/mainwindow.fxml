<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox maxWidth="1.7976931348623157E308" prefHeight="524.0" prefWidth="755.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mein.auth.service.MeinAuthAdminFX">
    <VBox maxHeight="1.7976931348623157E308" style="-fx-background-color: grey;">
        <Accordion BorderPane.alignment="CENTER">
            <panes>
                <TitledPane animated="true" text="MeinAuth">
                    <content>
                        <VBox>
                            <children>
                                <Button fx:id="btnGeneral" maxWidth="Infinity" mnemonicParsing="false" text="Info" />
                                <Button fx:id="btnOthers" maxWidth="Infinity" mnemonicParsing="false" text="Other Instances" />
                                <Button fx:id="btnDiscover" maxWidth="Infinity" mnemonicParsing="false" text="Discover" />
                                <Button fx:id="btnApprovals" maxWidth="Infinity" mnemonicParsing="false" text="Access" />
                            </children>
                            <rotationAxis>
                                <Point3D />
                            </rotationAxis>
                        </VBox>
                    </content>
                </TitledPane>
                <TitledPane fx:id="tpServices" animated="true" text="Services">
                    <content>
                        <VBox>
                            <ListView fx:id="serviceList" prefHeight="200.0" prefWidth="200.0">

                            </ListView>
                            <HBox alignment="TOP_RIGHT">
                                <Button fx:id="btnRemoveService" text="-" />
                                <Button fx:id="btnCreateService" text="+">
                                    <contextMenu>
                                        <ContextMenu fx:id="createServiceMenu">
                                            <items>
                                                <MenuItem mnemonicParsing="false" text="Unspecified Action" />
                                            </items>
                                        </ContextMenu>
                                    </contextMenu>
                                </Button>
                            </HBox>
                        </VBox>
                    </content>
                </TitledPane>
            </panes>
        </Accordion>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </VBox>
    <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <bottom>
            <HBox fx:id="hBoxButtons" alignment="CENTER" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                <Button fx:id="btnRefresh" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Refresh" HBox.hgrow="ALWAYS" />
                <Button fx:id="btnApply" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Apply" HBox.hgrow="ALWAYS" />
            </HBox>
        </bottom>
        <top>
            <Label fx:id="lblTitle" text="Title Goes Here">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
        </top>
        <center>
            <AnchorPane fx:id="paneContainer" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">

            </AnchorPane>
        </center>
    </BorderPane>
</HBox>
